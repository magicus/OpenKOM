<?xml version="1.0"?>
<!DOCTYPE faces-config PUBLIC
  "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.1//EN"
  "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">

<faces-config>
  <navigation-rule>
   <from-view-id>/listNews.faces</from-view-id>
    <navigation-case>
     <from-outcome>selectConference</from-outcome>
     <to-view-id>/listMessages.faces</to-view-id>
   </navigation-case>
  </navigation-rule>
  <managed-bean>
    <managed-bean-name>newsBean</managed-bean-name>
    <managed-bean-class>nu.rydin.kom.webui.beans.NewsBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
</faces-config>